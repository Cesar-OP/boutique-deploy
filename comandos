local
python manage.py runserver --settings=myshop.settings.local

set settings
set DJANGO_SETTINGS_MODULE=myshop.settings.local


ativar docker
docker run -it --rm --name rabbitmq -p 5672:5672 -p 15672:15672 rabbitmq:management

ativar celery
celery -A myshop worker -l info

#ativar redis
docker run -it --rm --name redis -p 6379:6379 redis

